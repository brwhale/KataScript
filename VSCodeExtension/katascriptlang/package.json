{
    "name": "katascriptlang",
    "displayName": "Kata Script Language Tools",
    "description": "Syntax Highlighting for KataScript",
    "publisher": "GarrettSkelton",
    "version": "0.0.62",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "katascript",
            "aliases": ["KataScript", "katascript", "Kata Script", "kata script"],
            "extensions": ["ks"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "katascript",
            "scopeName": "source.katascript",
            "path": "./syntaxes/katascript.tmLanguage.json"
        }]
    },
    "repository": { "url": "https://github.com/brwhale/KataScript"}
}